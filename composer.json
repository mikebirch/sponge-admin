{
    "name": "mikebirch/sponge-admin",
    "description": "SpongeAdmin plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "3.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SpongeAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpongeAdmin\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
