{
    "name": "mikebirch/sponge-admin",
    "description": "SpongeAdmin plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "mikebirch/sponge-cake": "master",
        "burzum/cakephp-user-tools": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SpongeAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpongeAdmin\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
